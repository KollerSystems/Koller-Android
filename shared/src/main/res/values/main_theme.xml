<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Theme.MainTheme" parent="Theme.Color">

        <item name="fontFamily">@font/rubik_family</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="bottomSheetDialogTheme">@style/CustomBottomSheetDialog</item>
        <item name="android:spinnerStyle">@style/Spinner</item>
        <item name="spinnerDropDownItemStyle">@style/SpinnerItem</item>
        <item name="tabStyle">@style/Tab</item>
        <item name="chipStyle">@style/ChipFilter</item>
        <item name="android:textViewStyle">@style/DefaultTextView</item>
        <item name="bottomSheetDragHandleStyle">@style/BottomSheetDrag</item>
        <item name="materialCardViewStyle">@style/SimpleCard</item>
        <item name="cardViewStyle">@style/SimpleCard</item>
        <item name="materialAlertDialogTheme">@style/Popup</item>
        <item name="toolbarStyle">@style/Toolbar</item>
        <item name="checkboxStyle">@style/Checkbox</item>
        <item name="radioButtonStyle">@style/Radio</item>
        <item name="floatingActionButtonStyle">@style/FloatingActionButton</item>
        <item name="android:windowSplashScreenBrandingImage" tools:targetApi="s">@drawable/company_logo</item>
        <item name="appBarLayoutStyle">@style/AppBarStyle</item>
        <item name="materialButtonStyle">@style/Button</item>
        <item name="android:navigationBarColor">?attr/colorSurfaceContainer</item>
        <item name="editTextStyle">@style/EditText</item>
        <item name="textInputStyle">@style/TextInput</item>
        <item name="recyclerViewStyle">@style/RecyclerView</item>
    </style>

    <style name="RecyclerView">
        <item name="android:fillViewport">true</item>
    </style>

    <style name="CardTextInput" parent="@style/TextInput">
        <item name="boxCornerRadiusBottomEnd">@dimen/card_corner_radius</item>
        <item name="boxCornerRadiusBottomStart">@dimen/card_corner_radius</item>
        <item name="boxCornerRadiusTopEnd">@dimen/card_corner_radius</item>
        <item name="boxCornerRadiusTopStart">@dimen/card_corner_radius</item>
    </style>

    <style name="ImageViewerDialog" parent="android:Theme.Translucent.NoTitleBar"/>

    <style name="ImageViewerDialog.Default">
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:windowTranslucentNavigation">true</item>
    </style>

    <style name="SimpleCardWithContent" parent="@style/SimpleCard">
        <item name="android:layout_margin">@dimen/text_container_margin</item>
        <item name="contentPadding">@dimen/card_padding</item>
        <item name="android:outlineProvider">none</item>
    </style>

    <style name="SeparatorTitle" parent="Text">
        <item name="android:textStyle">bold</item>
        <item name="android:layout_marginTop">@dimen/text_container_margin</item>
    </style>

    <style name="SeparatorTitleMain" parent="@style/Title">
        <item name="android:textStyle">bold</item>
        <item name="android:layout_marginTop">@dimen/n_text_container_margin</item>
        <item name="android:layout_marginStart">@dimen/n_text_container_margin</item>
    </style>

    <style name="TextInput" parent="@style/Widget.Material3.TextInputLayout.OutlinedBox">
         <item name="android:layout_marginBottom">@dimen/text_container_margin</item>
        <item name="android:layout_marginHorizontal">@dimen/text_container_margin</item>
        <item name="suffixTextAppearance">@style/Description</item>
    </style>

    <style name="EditText">
        <item name="android:textAppearance">@style/Text</item>
        <item name="android:drawablePadding">-15dp</item>
    </style>

    <style name="CardOutline" parent="@style/SimpleCard">
        <item name="android:outlineProvider">none</item>
        <item name="cardBackgroundColor">@android:color/transparent</item>
        <item name="strokeWidth">1dp</item>
        <item name="contentPadding">@dimen/card_padding</item>
        <item name="android:layout_margin">@dimen/text_container_margin</item>
    </style>



    <style name="SeparatorLine">
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">?attr/colorForeground01</item>
    </style>

    <style name="Separator" parent="@style/CategoryCardName">
        <item name="android:layout_marginTop">@null</item>
        <item name="android:background">?android:attr/colorBackground</item>
        <item name="android:paddingHorizontal">@dimen/card_padding</item>
        <item name="android:layout_marginHorizontal">@dimen/card_padding</item>
        <item name="android:paddingVertical">@dimen/text_container_margin</item>
        <item name="android:translationZ">1dp</item>
        <item name="android:outlineProvider">none</item>
        <item name="layout_maxWidth">@dimen/tablet_max_width</item>
    </style>

    <style name="HeaderCard">
        <item name="cardCornerRadius">@null</item>
        <item name="cardBackgroundColor">?attr/colorSurfaceContainer</item>
        <item name="shapeAppearance">@style/OverlayLargeRoundedCardTop</item>
    </style>

    <style name="Button" parent="@style/Widget.Material3.Button">
        <item name="android:paddingTop">@dimen/button_vertical_padding</item>
        <item name="android:paddingBottom">@dimen/button_vertical_padding</item>
        <item name="android:minWidth">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:minHeight">0dp</item>
        <item name="iconGravity">textStart</item>
    </style>

    <style name="ButtonTonal" parent="@style/Button">
        <item name="android:backgroundTint">?attr/colorSecondaryContainer</item>
        <item name="android:textColor">?attr/colorOnSecondaryContainer</item>
        <item name="iconTint">?attr/colorOnSecondaryContainer</item>
    </style>

    <style name="ButtonTonalIcon" parent="@style/ButtonTonal">
        <item name="android:paddingStart">16dp</item>
    </style>

    <style name="ButtonOutline" parent="@style/Button">
        <item name="backgroundTint">@android:color/transparent</item>
        <item name="strokeColor" tools:ignore="PrivateResource">@color/m3_button_outline_color_selector</item>
        <item name="android:textColor">?android:attr/colorPrimary</item>
        <item name="strokeWidth">1dp</item>
    </style>

    <style name="ButtonIcon" parent="@style/Button">
        <item name="android:paddingStart">16dp</item>
    </style>

    <style name="ButtonOutlineIcon" parent="@style/ButtonOutline">
        <item name="android:paddingStart">16dp</item>
        <item name="iconTint">?android:attr/colorPrimary</item>
    </style>

    <style name="IconButton" parent="@style/Button">
        <item name="android:paddingStart">@dimen/button_vertical_padding</item>
        <item name="android:paddingEnd">@dimen/button_vertical_padding</item>
        <item name="iconPadding">0dp</item>
    </style>

    <style name="IconButtonTransparent" parent="@style/Widget.Material3.Button.IconButton">
        <item name="android:paddingTop">@dimen/button_vertical_padding</item>
        <item name="android:paddingBottom">@dimen/button_vertical_padding</item>
        <item name="android:minWidth">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="android:insetLeft">0dp</item>
        <item name="android:insetRight">0dp</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:minHeight">0dp</item>
        <item name="android:paddingStart">@dimen/button_vertical_padding</item>
        <item name="android:paddingEnd">@dimen/button_vertical_padding</item>
        <item name="iconPadding">0dp</item>
    </style>

    <style name="IconButtonOutline" parent="@style/ButtonOutlineIcon">
        <item name="android:paddingStart">@dimen/button_vertical_padding</item>
        <item name="android:paddingEnd">@dimen/button_vertical_padding</item>
        <item name="iconPadding">0dp</item>
    </style>

    <style name="IconButtonTonal" parent="@style/ButtonTonalIcon">
        <item name="android:paddingStart">@dimen/button_vertical_padding</item>
        <item name="android:paddingEnd">@dimen/button_vertical_padding</item>
        <item name="iconPadding">0dp</item>
    </style>

    <style name="AppBarStyle">
        <item name="android:layout_height">@dimen/expanded_toolbar_height</item>
    </style>

    <style name="FloatingActionButton" parent="@style/Widget.Material3.FloatingActionButton.Primary">
        <item name="android:layout_gravity">bottom|end</item>
        <item name="android:layout_margin">@null</item>
        <item name="android:layout_marginStart">@dimen/application_padding</item>
        <item name="android:layout_marginBottom">@dimen/application_padding</item>
        <item name="android:backgroundTint">?attr/colorSecondaryContainer</item>
        <item name="android:tint">?attr/colorOnSecondaryContainer</item>
        <item name="useCompatPadding">false</item>
    </style>

    <style name="Checkbox" parent="@style/Widget.Material3.CompoundButton.CheckBox">
        <item name="android:minHeight">0dp</item>
        <item name="android:minWidth">0dp</item>
        <item name="android:textAppearance">@style/Text</item>
        <item name="android:paddingStart">@dimen/text_container_margin</item>
    </style>

    <style name="Radio" parent="@style/Widget.Material3.CompoundButton.RadioButton">
        <item name="android:minHeight">0dp</item>
        <item name="android:minWidth">0dp</item>
        <item name="android:textAppearance">@style/Text</item>
        <item name="android:paddingStart">@dimen/text_container_margin</item>
    </style>

    <style name="TrickyToolbar">
        <item name="android:paddingHorizontal">@dimen/application_padding</item>
        <item name="android:paddingTop">@dimen/application_padding</item>
        <item name="layout_scrollFlags">scroll</item>
    </style>

    <style name="HorizontalUserRecyclerView" parent="@style/MainCardLayout">
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="layoutManager">androidx.recyclerview.widget.LinearLayoutManager</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingHorizontal">20dp</item>
        <item name="android:paddingVertical">@dimen/text_container_margin</item>
        <item name="android:layout_marginStart">@dimen/n_card_padding</item>
        <item name="android:layout_marginEnd">@dimen/n_card_padding</item>
    </style>
    
    <style name="ImageAddButton" parent="Button">
        <item name="backgroundTint">?attr/colorSecondary</item>
        <item name="android:layout_width">@dimen/image_list_height</item>
        <item name="android:layout_height">@dimen/image_list_height</item>
        <item name="android:layout_margin">@dimen/text_container_margin</item>
        <item name="android:text">@string/plus_image</item>
        <item name="android:textColor">?attr/colorOnSecondary</item>
        <item name="android:textSize">20sp</item>
        <item name="cornerRadius">15dp</item>
    </style>
    
    <style name="SimpleCard" parent="@style/Widget.Material3.CardView.Filled">
        <item name="cardElevation">0dp</item>
        <item name="cardCornerRadius">@dimen/card_corner_radius</item>
        <item name="cardBackgroundColor">?attr/colorSurfaceContainerLow</item>
        <item name="cardPreventCornerOverlap">false</item>
    </style>

    <style name="BottomSheetDrag" parent="Widget.Material3.BottomSheet.DragHandle">
        <item name="tint">?attr/colorForeground066</item>
        <item name="android:minHeight">0dp</item>
        <item name="android:paddingBottom">12dp</item>
        <item name="android:paddingTop">12dp</item>
    </style>

    <style name="CategoryIcon" parent="@style/Icon">
        <item name="android:layout_width">30dp</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_marginTop">15dp</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:alpha">0.666</item>
        <item name="android:layout_marginEnd">@dimen/spacing</item>
    </style>

    <style name="DefaultTextView">
        <item name="android:textAppearance">@style/Text</item>
    </style>

    <style name="ChipFilter" parent="@style/Widget.Material3.Chip.Filter">
        <item name="checkedIconEnabled">false</item>
        <item name="checkedIcon">@null</item>
        <item name="chipMinTouchTargetSize">0dp</item>
        <item name="chipMinHeight">0dp</item>
        <item name="android:paddingVertical">8dp</item>
        <item name="android:textAppearance">@style/Description</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="IconLikeText">
        <item name="autoSizeTextType">uniform</item>
        <item name="android:scaleX">1.2</item>
        <item name="android:scaleY">1.2</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_height">@dimen/card_icon</item>
        <item name="android:layout_width">@dimen/card_icon</item>
    </style>

    <style name="ChipDropdownFilter" parent="@style/ChipFilter">
        <item name="closeIcon">@drawable/arrow_drop</item>
        <item name="android:checkable">false</item>
        <item name="closeIconEnabled">true</item>
    </style>

    <style name="Spinner" parent="android:Widget.Material.Spinner">
        <item name="android:popupBackground">@drawable/panel_dropdown</item>
    </style>

    <style name="SpinnerItem" parent="@style/Text">
        <item name="android:paddingHorizontal">20dp</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="RatingBar" parent="@android:style/Widget.Material.RatingBar.Small">
        <item name="android:progressTint">?attr/colorPrimary</item>
        <item name="android:secondaryProgressTint">?attr/colorPrimary</item>
        <item name="android:progressBackgroundTint">?attr/colorPrimary</item>
    </style>

    <style name="Popup" parent="ThemeOverlay.Material3.MaterialAlertDialog.Centered">
        <item name="materialAlertDialogTitleTextStyle">@style/DialogTitle</item>
    </style>

    <style name="DialogTitle" parent="MainTitle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAlignment">center</item>
    </style>


    <style name="CardButton" parent="@style/ButtonIcon">
         <item name="android:paddingTop">@dimen/card_padding</item>
        <item name="android:paddingBottom">@dimen/card_padding</item>
        <item name="android:backgroundTint">?attr/colorSurfaceContainerLow</item>
        <item name="cornerRadius">0dp</item>
        <item name="android:elevation">0dp</item>
        <item name="iconGravity">textStart</item>
        <item name="iconTint">?android:attr/colorForeground</item>
        <item name="android:textColor">?android:attr/colorForeground</item>
        <item name="android:textSize">@dimen/text</item>
    </style>

    <style name="MainTitleExpanded">
        <item name="android:textSize">@dimen/text_main_title_expanded</item>
        <item name="android:textColor">?android:attr/colorForeground</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="MainTitle">
        <item name="android:textSize">@dimen/text_main_title</item>
        <item name="android:textColor">?android:attr/colorForeground</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="Title">
        <item name="android:textSize">@dimen/text_title</item>
        <item name="android:textColor">?android:attr/colorForeground</item>
    </style>

    <style name="SmallTitle">
        <item name="android:textSize">@dimen/text</item>
        <item name="android:textColor">?android:attr/colorForeground</item>
        <item name="android:textStyle">bold</item>
        <item name="minHeight">15dp</item>
    </style>

    <style name="Text">
        <item name="android:textSize">@dimen/text</item>
        <item name="android:textColor">?android:attr/colorForeground</item>
    </style>

    <style name="CategoryCardName" parent="@style/Text">
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">?attr/colorForeground066</item>
        <item name="android:paddingHorizontal">@dimen/card_padding</item>
    </style>

    <style name="CategoryCardNameBald" parent="@style/CategoryCardName">
        <item name="android:paddingHorizontal">@dimen/spacing</item>
    </style>

    <style name="Description">
        <item name="android:textSize">@dimen/text_description</item>
        <item name="android:textColor">?colorForeground066</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="RoundImageView">
        <item name="cornerSize">50%</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="OverlayRoundedLeft">
        <item name="cornerSizeBottomLeft">8dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerSizeTopLeft">8dp</item>
        <item name="cornerSizeTopRight">0dp</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="OverlayRoundedRight">
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeBottomRight">8dp</item>
        <item name="cornerSizeTopLeft">0dp</item>
        <item name="cornerSizeTopRight">8dp</item>
        <item name="cornerFamily">rounded</item>
    </style>


    <style name="TextButton" parent="@style/Widget.Material3.Button.TextButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="android:minWidth">0dp</item>
        <item name="android:minHeight">0dp</item>
        <item name="android:paddingHorizontal">12dp</item>
        <item name="android:paddingVertical">4dp</item>
        <item name="android:paddingStart">@null</item>
        <item name="android:paddingEnd">@null</item>
        <item name="android:paddingBottom">@null</item>
        <item name="android:paddingTop">@null</item>
        <item name="android:textAppearance">@style/Description</item>
        <item name="iconGravity">textStart</item>
    </style>

    <style name="OverlayRoundedCardLeft">
        <item name="cornerSizeBottomLeft">@dimen/card_corner_radius</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerSizeTopLeft">@dimen/card_corner_radius</item>
        <item name="cornerSizeTopRight">0dp</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="OverlayRoundedCardRight">
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeBottomRight">@dimen/card_corner_radius</item>
        <item name="cornerSizeTopLeft">0dp</item>
        <item name="cornerSizeTopRight">@dimen/card_corner_radius</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="OverlaySmallRoundedCardLeft">
        <item name="cornerSizeBottomLeft">@dimen/card_corner_radius_small</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerSizeTopLeft">@dimen/card_corner_radius</item>
        <item name="cornerSizeTopRight">0dp</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="OverlaySmallRoundedCardRight">
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeBottomRight">@dimen/card_corner_radius_small</item>
        <item name="cornerSizeTopLeft">0dp</item>
        <item name="cornerSizeTopRight">@dimen/card_corner_radius_small</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="OverlaySmallRoundedCardTopLeft">
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerSizeTopLeft">@dimen/card_corner_radius_small</item>
        <item name="cornerSizeTopRight">0dp</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="OverlaySmallRoundedCardTopRight">
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerSizeTopLeft">0dp</item>
        <item name="cornerSizeTopRight">@dimen/card_corner_radius_small</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="OverlaySmallRoundedCardTop">
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerSizeTopLeft">@dimen/card_corner_radius_small</item>
        <item name="cornerSizeTopRight">@dimen/card_corner_radius_small</item>
        <item name="cornerFamily">rounded</item>
    </style>


    <style name="OverlaySmallRoundedCard">
        <item name="cornerSizeBottomLeft">@dimen/card_corner_radius_small</item>
        <item name="cornerSizeBottomRight">@dimen/card_corner_radius_small</item>
        <item name="cornerSizeTopLeft">@dimen/card_corner_radius_small</item>
        <item name="cornerSizeTopRight">@dimen/card_corner_radius_small</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="OverlayRoundedCardTop">
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerSizeTopLeft">@dimen/card_corner_radius</item>
        <item name="cornerSizeTopRight">@dimen/card_corner_radius</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="OverlayLargeRoundedCardTop">
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerSizeTopLeft">@dimen/application_corner_radius</item>
        <item name="cornerSizeTopRight">@dimen/application_corner_radius</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="OverlayRoundedCardTopLeft">
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerSizeTopLeft">@dimen/card_corner_radius</item>
        <item name="cornerSizeTopRight">0dp</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="OverlayRoundedCardTopRight">
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerSizeTopLeft">0dp</item>
        <item name="cornerSizeTopRight">@dimen/card_corner_radius</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="OverlayRoundedCardBottom">
        <item name="cornerSizeBottomLeft">@dimen/card_corner_radius</item>
        <item name="cornerSizeBottomRight">@dimen/card_corner_radius</item>
        <item name="cornerSizeTopLeft">0dp</item>
        <item name="cornerSizeTopRight">0dp</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="OverlayRoundedCardBottomLeft">
        <item name="cornerSizeBottomLeft">@dimen/card_corner_radius</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerSizeTopLeft">0dp</item>
        <item name="cornerSizeTopRight">0dp</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="OverlayRoundedCardBottomRight">
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeBottomRight">@dimen/card_corner_radius</item>
        <item name="cornerSizeTopLeft">0dp</item>
        <item name="cornerSizeTopRight">0dp</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="OverlayRoundedCard">
        <item name="cornerSizeBottomLeft">@dimen/card_corner_radius</item>
        <item name="cornerSizeBottomRight">@dimen/card_corner_radius</item>
        <item name="cornerSizeTopLeft">@dimen/card_corner_radius</item>
        <item name="cornerSizeTopRight">@dimen/card_corner_radius</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="Image">
        <item name="android:scaleType">centerCrop</item>
        <item name="shapeAppearanceOverlay">@style/RoundedImageView</item>
    </style>

    <style name="RoundedImageView">
        <item name="cornerSize">10dp</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="Card" parent="@style/SimpleCard">
        <item name="contentPadding">@dimen/card_padding</item>
        <item name="android:clipChildren">false</item>
        <item name="android:clipToPadding">false</item>
        <item name="cardCornerRadius">@null</item>
        <item name="shapeAppearance">@null</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:outlineProvider">none</item>
        <item name="cardPreventCornerOverlap">false</item>
    </style>

    <style name="CardTop" parent="@style/Card">
        <item name="shapeAppearance">@style/OverlayRoundedCardTop</item>
        <item name="android:layout_marginBottom">@dimen/card_margin</item>
        <item name="android:layout_marginTop">@dimen/text_container_margin</item>
    </style>
    <style name="CardMiddle" parent="@style/Card">
        <item name="android:layout_marginVertical">@dimen/card_margin</item>
    </style>
    <style name="CardBottom" parent="@style/Card">
        <item name="shapeAppearance">@style/OverlayRoundedCardBottom</item>
        <item name="android:layout_marginTop">@dimen/card_margin</item>
