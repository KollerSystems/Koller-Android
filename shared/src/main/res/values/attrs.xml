    <attr name="text" format="string" />
    <attr name="icon" format="reference" />

    <declare-styleable name="simpleCardButton">
        <attr name="swap_description_and_text" format="boolean"/>
        <attr name="rounded" format="boolean"/>
        <attr name="text"/>
        <attr name="description" format="string" />
        <attr name="badge" format="integer" />
        <attr name="icon" />
        <attr name="end_icon" format="reference" />
    </declare-styleable>

    <declare-styleable name="tipView">
        <attr name="text"/>
        <attr name="type" format="enum">
            <enum name="tip" value="0"/>
            <enum name="help" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="eventPoint">
        <attr name="text"/>
        <attr name="time" format="string" />
    </declare-styleable>

    <declare-styleable name="nameContentView">
        <attr name="name" format="string" />
        <attr name="detail" format="string" />
        <attr name="icon" />
    </declare-styleable>

    <declare-styleable name="roundedBadgeImageView">
        <attr name="stroke_width" format="dimension" />
    </declare-styleable>


