                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginVertical="@dimen/text_container_margin"
                    android:layout_marginStart="@dimen/text_container_margin"
                    android:layout_weight="1"
                    android:paddingVertical="17dp"
                    android:text="@string/woman"
                    android:textAlignment="center"
                    android:textSize="14sp"
                    app:chipEndPadding="0dp"
                    app:chipStartPadding="0dp"
                    app:shapeAppearance="@style/OverlayRoundedLeft"
                    app:textEndPadding="0dp"
                    app:textStartPadding="0dp" />

                <com.google.android.material.chip.Chip
                    android:id="@+id/chip3"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginVertical="@dimen/text_container_margin"
                    android:layout_marginEnd="@dimen/text_container_margin"
                    android:layout_weight="1"
                    android:paddingVertical="17dp"
                    android:text="@string/man"
                    android:textAlignment="center"
                    android:textSize="14sp"
                    app:chipEndPadding="0dp"
                    app:chipStartPadding="0dp"
                    app:shapeAppearance="@style/OverlayRoundedRight"
                    app:textEndPadding="0dp"
                    app:textStartPadding="0dp" />

            </LinearLayout>

        </com.google.android.material.card.MaterialCardView>

        <com.google.android.material.card.MaterialCardView
