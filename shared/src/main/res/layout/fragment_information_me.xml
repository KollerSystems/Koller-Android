                    android:text="@string/introvert_description"
                    android:textAppearance="@style/Description"
                    app:drawableLeftCompat="@drawable/info" />

                <TextView
                    android:id="@+id/textView25"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/text_container_margin"
                    android:drawablePadding="@dimen/text_container_margin"
                    android:text="@string/extrovert_description"
                    android:textAppearance="@style/Description"
