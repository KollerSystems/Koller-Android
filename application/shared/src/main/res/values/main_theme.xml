<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Theme.MainTheme" parent="Theme.Color">

        <item name="fontFamily">@font/rubik_family</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="bottomSheetDialogTheme">@style/CustomBottomSheetDialog</item>
        <item name="android:spinnerStyle">@style/kollerSpinnerStyle</item>
        <item name="spinnerDropDownItemStyle">@style/kollerSpinnerDropdownItemStyle</item>
        <item name="android:spinnerItemStyle">@style/kollerSpinnerItemStyle</item>
        <item name="tabStyle">@style/tab</item>
        <item name="chipStyle">@style/chipButtonFilterStyle</item>
        <item name="android:textViewStyle">@style/DefaultTextView</item>
        <item name="bottomSheetDragHandleStyle">@style/bottomSheetDrag</item>
        <item name="materialCardViewStyle">@style/SimpleCard</item>
        <item name="materialAlertDialogTheme">@style/Popup</item>
        <item name="toolbarStyle">@style/toolbar</item>
        <item name="checkboxStyle">@style/Checkbox</item>
        <item name="floatingActionButtonStyle">@style/floatingActionButtonC</item>
        <item name="android:windowSplashScreenBrandingImage" tools:targetApi="s">@drawable/company_logo</item>
        <item name="appBarLayoutStyle">@style/appBarStyle</item>
        <item name="materialButtonStyle">@style/button</item>
        <item name="android:navigationBarColor">@android:color/transparent</item>
    </style>

    <style name="main_background" parent="@style/Widget.Material3.CardView.Filled">
        <item name="cardBackgroundColor">?attr/colorSurface</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">match_parent</item>
        <item name="cardCornerRadius">0dp</item>
        <item name="cardElevation">@dimen/header_footer_elevation</item>

    </style>

    <style name="separator_line">
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">?attr/colorForeground01</item>
    </style>

    <style name="separator" parent="@style/CategoryCardName">
        <item name="android:layout_marginHorizontal">@null</item>
        <item name="android:layout_marginTop">@null</item>
        <item name="android:background">?android:attr/windowBackground</item>
        <item name="android:paddingHorizontal">30dp</item>
        <item name="android:paddingVertical">@dimen/text_container_margin</item>
        <item name="android:translationZ">1dp</item>
        <item name="android:outlineProvider">none</item>
    </style>

    <style name="button" parent="@style/Widget.Material3.Button">
        <item name="android:paddingTop">@dimen/button_vertical_padding</item>
        <item name="android:paddingBottom">@dimen/button_vertical_padding</item>
        <item name="android:minWidth">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:minHeight">0dp</item>
    </style>

    <style name="button_tonal" parent="@style/button">
        <item name="android:backgroundTint">?attr/colorSecondaryContainer</item>
        <item name="android:textColor">?attr/colorOnSecondaryContainer</item>
    </style>

    <style name="button_outline" parent="@style/button">
        <item name="backgroundTint">@android:color/transparent</item>
        <item name="strokeColor">@color/m3_button_outline_color_selector</item>
        <item name="android:textColor">?android:attr/colorPrimary</item>
        <item name="strokeWidth">1dp</item>
    </style>

    <style name="button_icon" parent="@style/button">
        <item name="android:paddingLeft">16dp</item>
    </style>

    <style name="button_outline_icon" parent="@style/button_outline">
        <item name="android:paddingLeft">16dp</item>
        <item name="iconTint">?android:attr/colorPrimary</item>
    </style>

    <style name="icon_button" parent="@style/button">
        <item name="android:paddingLeft">@dimen/button_vertical_padding</item>
        <item name="android:paddingRight">@dimen/button_vertical_padding</item>
        <item name="iconPadding">0dp</item>
    </style>

    <style name="icon_button_transparent" parent="@style/icon_button">
        <item name="android:background">@android:color/transparent</item>
    </style>

    <style name="icon_button_outline" parent="@style/button_outline_icon">
        <item name="android:paddingLeft">@dimen/button_vertical_padding</item>
        <item name="android:paddingRight">@dimen/button_vertical_padding</item>
        <item name="iconPadding">0dp</item>
    </style>

    <style name="appBarStyle">
        <item name="background">@null</item>
    </style>

    <style name="floatingActionButtonC" parent="@style/Widget.Material3.FloatingActionButton.Primary">
        <item name="android:layout_gravity">bottom|right</item>
        <item name="android:layout_margin">@null</item>
        <item name="android:layout_marginLeft">@dimen/application_padding</item>
        <item name="android:layout_marginBottom">@dimen/application_padding</item>
        <item name="android:backgroundTint">?attr/colorSecondaryContainer</item>
        <item name="android:tint">?attr/colorOnSecondaryContainer</item>
        <item name="useCompatPadding">false</item>
    </style>

    <style name="Checkbox" parent="@style/Widget.Material3.CompoundButton.CheckBox">
        <item name="android:minHeight">0dp</item>
        <item name="android:minWidth">0dp</item>
    </style>

    <style name="tricky_toolbar">
        <item name="android:paddingHorizontal">@dimen/application_padding</item>
        <item name="android:paddingTop">@dimen/application_padding</item>
        <item name="layout_scrollFlags">scroll</item>
    </style>

    <style name="HorizontalUserRecyclerView" parent="@style/MainCardLayout">
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="layoutManager">androidx.recyclerview.widget.LinearLayoutManager</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingHorizontal">20dp</item>
        <item name="android:paddingVertical">@dimen/text_container_margin</item>
        <item name="android:layout_marginLeft">@dimen/n_panel_padding</item>
        <item name="android:layout_marginRight">@dimen/n_panel_padding</item>
    </style>
    
    <style name="ImageAddButton" parent="button">
        <item name="backgroundTint">?attr/colorSecondary</item>
        <item name="android:layout_width">@dimen/imageListHeight</item>
        <item name="android:layout_height">@dimen/imageListHeight</item>
        <item name="android:layout_margin">@dimen/CardMargin</item>
        <item name="android:text">@string/plus_image</item>
        <item name="android:textColor">?attr/colorOnSecondary</item>
        <item name="android:textSize">20sp</item>
        <item name="cornerRadius">15dp</item>
    </style>
    
    <style name="SimpleCard" parent="@style/Widget.Material3.CardView.Filled">
        <item name="cardElevation">@dimen/default_card_elevation</item>
        <item name="cardCornerRadius">@dimen/panel_round</item>
        <item name="cardBackgroundColor">?attr/colorSurface</item>
    </style>

    <style name="bottomSheetDrag" parent="Widget.Material3.BottomSheet.DragHandle">
        <item name="tint">?attr/colorForeground066</item>
    </style>

    <style name="CategoryIcon" parent="@style/Icon">
        <item name="android:layout_width">30dp</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_marginTop">15dp</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:alpha">0.666</item>
        <item name="android:layout_marginRight">@dimen/spacing</item>
    </style>

    <style name="DefaultTextView">
        <item name="android:textAppearance">@style/Normal</item>
    </style>

    <style name="kollerSpinnerItemStyle" parent="@style/Normal">

    </style>

    <style name="chipButtonFilterStyle" parent="@style/Widget.Material3.Chip.Filter">
        <item name="android:textAppearance">@style/chipButtonTextStyle</item>
        <item name="checkedIconEnabled">false</item>
        <item name="checkedIcon">@null</item>
        <item name="chipMinTouchTargetSize">0dp</item>
        <item name="chipMinHeight">0dp</item>
        <item name="android:paddingVertical">8dp</item>
    </style>

    <style name="iconLikeText">
        <item name="autoSizeTextType">uniform</item>
        <item name="android:scaleX">1.2</item>
        <item name="android:scaleY">1.2</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_height">@dimen/panel_icon</item>
        <item name="android:layout_width">@dimen/panel_icon</item>
    </style>

    <style name="chipDropdownButtonFilterStyle" parent="@style/chipButtonFilterStyle">
        <item name="closeIcon">@drawable/arrow_drop</item>
        <item name="closeIconEnabled">true</item>
    </style>

    <style name="chipButtonTextStyle" parent="@style/Description">

    </style>

    <style name="kollerSpinnerStyle" parent="android:Widget.Material.Spinner">
            <item name="android:popupBackground">@drawable/panel_dropdown</item>
    </style>

    <style name="kollerSpinnerDropdownItemStyle" parent="@style/Normal">
        <item name="android:paddingHorizontal">20dp</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="RatingBar" parent="@android:style/Widget.Material.RatingBar.Small">
        <item name="android:progressTint">?attr/colorPrimary</item>
        <item name="android:secondaryProgressTint">?attr/colorPrimary</item>
        <item name="android:progressBackgroundTint">?attr/colorPrimary</item>
    </style>

    <style name="ImagePopup" parent="ThemeOverlay.Material3.MaterialAlertDialog.Centered">
        <item name="materialAlertDialogTitleTextStyle">@style/DialogTitleWithImage</item>
        <item name="materialAlertDialogTitleIconStyle">@style/DialogIcon</item>
    </style>

    <style name="Popup" parent="ThemeOverlay.Material3.MaterialAlertDialog.Centered">
        <item name="materialAlertDialogTitleTextStyle">@style/DialogTitle</item>
    </style>

    <style name="DialogTitle" parent="MainTitle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAlignment">center</item>
    </style>
    <style name="DialogTitleWithImage" parent="DialogTitle">
        <item name="android:translationY">-200dp</item>
        <item name="android:paddingTop">15dp</item>
    </style>

    <style name="DialogIcon">
        <item name="android:layout_height">200dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:translationY">50dp</item>
        <item name="android:adjustViewBounds">true</item>
    </style>


    <style name="panelButton" parent="@style/button">
         <item name="android:paddingTop">@dimen/panel_padding</item>
        <item name="android:paddingBottom">@dimen/panel_padding</item>
        <item name="android:backgroundTint">?attr/colorSurface</item>
        <item name="cornerRadius">0dp</item>
        <item name="android:elevation">@dimen/default_card_elevation</item>
        <item name="iconGravity">textStart</item>
        <item name="iconTint">?android:attr/colorForeground</item>
        <item name="android:textColor">?android:attr/colorForeground</item>
        <item name="android:textSize">@dimen/text_normal</item>
    </style>

    <style name="MainTitleExpanded">
        <item name="android:textSize">@dimen/text_main_title_expanded</item>
        <item name="android:textColor">?android:attr/colorForeground</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="MainTitle">
        <item name="android:textSize">@dimen/text_main_title</item>
        <item name="android:textColor">?android:attr/colorForeground</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="Title">
        <item name="android:textSize">@dimen/text_title</item>
        <item name="android:textColor">?android:attr/colorForeground</item>
    </style>

    <style name="SmallTitle">
        <item name="android:textSize">@dimen/text_normal</item>
        <item name="android:textColor">?android:attr/colorForeground</item>
        <item name="android:textStyle">bold</item>
        <item name="minHeight">15dp</item>
    </style>

    <style name="Normal">
        <item name="android:textSize">@dimen/text_normal</item>
        <item name="android:textColor">?android:attr/colorForeground</item>
    </style>

    <style name="CategoryCardName" parent="Normal">
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">?attr/colorForeground066</item>
        <item name="android:layout_marginHorizontal">15dp</item>
    </style>

    <style name="Description">
        <item name="android:textSize">@dimen/text_description</item>
        <item name="android:textColor">?colorForeground066</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="roundImageView">
        <item name="cornerSize">50%</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="overlayRoundedLeft">
        <item name="cornerSizeBottomLeft">8dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerSizeTopLeft">8dp</item>
        <item name="cornerSizeTopRight">0dp</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="overlayRoundedRight">
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeBottomRight">8dp</item>
        <item name="cornerSizeTopLeft">0dp</item>
        <item name="cornerSizeTopRight">8dp</item>
        <item name="cornerFamily">rounded</item>
    </style>


    <style name="textButton" parent="@style/Widget.Material3.Button.TextButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="android:minWidth">0dp</item>
        <item name="android:minHeight">0dp</item>
        <item name="android:paddingHorizontal">12dp</item>
        <item name="android:paddingVertical">4dp</item>
        <item name="android:paddingLeft">0dp</item>
        <item name="android:paddingRight">0dp</item>
        <item name="android:paddingBottom">0dp</item>
        <item name="android:paddingTop">0dp</item>
        <item name="android:textAppearance">@style/Description</item>
    </style>

    <style name="overlayRoundedCardLeft">
        <item name="cornerSizeBottomLeft">@dimen/panel_round</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerSizeTopLeft">@dimen/panel_round</item>
        <item name="cornerSizeTopRight">0dp</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="overlayRoundedCardRight">
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeBottomRight">@dimen/panel_round</item>
        <item name="cornerSizeTopLeft">0dp</item>
        <item name="cornerSizeTopRight">@dimen/panel_round</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="overlaySmallRoundedCardLeft">
        <item name="cornerSizeBottomLeft">@dimen/panel_small_radius</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerSizeTopLeft">@dimen/panel_round</item>
        <item name="cornerSizeTopRight">0dp</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="overlaySmallRoundedCardRight">
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeBottomRight">@dimen/panel_small_radius</item>
        <item name="cornerSizeTopLeft">0dp</item>
        <item name="cornerSizeTopRight">@dimen/panel_small_radius</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="overlaySmallRoundedCardTopLeft">
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerSizeTopLeft">@dimen/panel_small_radius</item>
        <item name="cornerSizeTopRight">0dp</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="overlaySmallRoundedCardTopRight">
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerSizeTopLeft">0dp</item>
        <item name="cornerSizeTopRight">@dimen/panel_small_radius</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="overlaySmallRoundedCardTop">
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerSizeTopLeft">@dimen/panel_small_radius</item>
        <item name="cornerSizeTopRight">@dimen/panel_small_radius</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="overlaySmallRoundedCard">
        <item name="cornerSizeBottomLeft">@dimen/panel_small_radius</item>
        <item name="cornerSizeBottomRight">@dimen/panel_small_radius</item>
        <item name="cornerSizeTopLeft">@dimen/panel_small_radius</item>
        <item name="cornerSizeTopRight">@dimen/panel_small_radius</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="overlayRoundedCardTop">
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerSizeTopLeft">@dimen/panel_round</item>
        <item name="cornerSizeTopRight">@dimen/panel_round</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="overlayRoundedCardBottom">
        <item name="cornerSizeBottomLeft">@dimen/panel_round</item>
        <item name="cornerSizeBottomRight">@dimen/panel_round</item>
        <item name="cornerSizeTopLeft">0dp</item>
        <item name="cornerSizeTopRight">0dp</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="overlayRoundedCard">
        <item name="cornerSizeBottomLeft">@dimen/panel_round</item>
        <item name="cornerSizeBottomRight">@dimen/panel_round</item>
        <item name="cornerSizeTopLeft">@dimen/panel_round</item>
        <item name="cornerSizeTopRight">@dimen/panel_round</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="roundedImageView">
        <item name="cornerSize">10dp</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="chipGroupArrangeCard">
        <item name="cardCornerRadius">8dp</item>
        <item name="android:layout_marginVertical">8dp</item>
        <item name="strokeWidth">1dp</item>
    </style>

    <style name="chipGroupArrange" parent="@style/Widget.Material3.ChipGroup">
        <item name="chipSpacingHorizontal">0dp</item>
        <item name="chipSpacingVertical">0dp</item>
        <item name="selectionRequired">true</item>
        <item name="singleSelection">true</item>
    </style>

    <style name="chipArrange" parent="chipButtonFilterStyle">
        <item name="chipCornerRadius">0dp</item>
        <item name="chipMinTouchTargetSize">0dp</item>
    </style>

    <style name="Card" parent="@style/SimpleCard">
        <item name="contentPadding">@dimen/panel_padding</item>
        <item name="android:clipChildren">false</item>
        <item name="android:clipToPadding">false</item>
        <item name="cardCornerRadius">@null</item>
        <item name="shapeAppearance">@null</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:outlineProvider">none</item>
        <item name="cardPreventCornerOverlap">false</item>
    </style>

    <style name="CardHeader" parent="Card">
        <item name="android:layout_margin">0dp</item>
        <item name="contentPadding">0dp</item>
    </style>


    <style name="CardMainCategory" parent="Card">
        <item name="shapeAppearanceOverlay">@style/ShapeAppearanceOverlay.MaterialCardView.Cut</item>
    </style>


    <style name="ShapeAppearanceOverlay.MaterialCardView.Cut" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopRight">0dp</item>
        <item name="cornerSizeTopLeft">0dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerSizeBottomLeft">@dimen/panel_round</item>
    </style>

    <style name="MainCard" parent="@style/SimpleCard">
        <item name="cardBackgroundColor">@null</item>
        <item name="android:backgroundTint">#00000000</item>
        <item name="cardElevation">0dp</item>
        <item name="android:clipToPadding">false</item>
        <item name="android:clipChildren">false</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="MainCardLayout">
        <item name="android:showDividers">middle</item>
        <item name="android:divider">@drawable/divider</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:clipToPadding">false</item>
        <item name="android:clipChildren">false</item>

    </style>
    
    <style name="BottomSheetMainLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">0dp</item>
        <item name="android:layout_marginRight">0dp</item>
        <item name="android:layout_marginTop">0dp</item>
        <item name="cardCornerRadius">@dimen/application_round</item>
        <item name="cardElevation" >-16dp</item>
    </style>

    <style name="ToolbarNavigationButton" parent="SimpleButton">
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_margin">@dimen/text_container_margin</item>
        <item name="android:alpha">0.666</item>
        <item name="cornerRadius">9999dp</item>
        <item name="icon">@drawable/arrow_back</item>
        <item name="iconGravity">textStart</item>
        <item name="iconPadding">0dp</item>
        <item name="iconSize">25dp</item>
        <item name="iconTint">?attr/colorForeground</item>
        <item name="minWidth">0dp</item>
    </style>
    
    <style name="CornerRadius15">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopRight">15dp</item>
        <item name="cornerSizeTopLeft">15dp</item>
        <item name="cornerSizeBottomRight">15dp</item>
        <item name="cornerSizeBottomLeft">15dp</item>
    </style>

    <style name="SimpleButton" parent="@style/Widget.Material3.Button.TextButton">
        <item name="cornerRadius">0dp</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="android:padding">0dp</item>
        <item name="android:textColor">?android:attr/colorForeground</item>
        <item name="iconTint">?android:attr/colorForeground</item>
        <item name="android:minWidth">0dp</item>
        <item name="android:minHeight">0dp</item>
        <item name="iconGravity">textStart</item>
    </style>

    <style name="Icon">
        <item name="android:adjustViewBounds">true</item>
        <item name="tint">?android:attr/colorForeground</item>
    </style>

    <style name="toolbar" parent="@style/Widget.Material3.Toolbar">
        <item name="android:layout_height">@dimen/header_footer_size</item>
        <item name="android:minHeight">0dp</item>
    </style>

    <style name="collapsing_toolbar_layout" parent="@style/Widget.Material3.CollapsingToolbar">
        <item name="layout_scrollFlags">scroll|exitUntilCollapsed|snap</item>
        <item name="collapsedTitleTextAppearance">@style/MainTitle</item>
        <item name="expandedTitleGravity">center</item>
        <item name="expandedTitleTextAppearance">@style/MainTitleExpanded</item>
        <item name="scrimVisibleHeightTrigger">@dimen/expandedToolbarHeight</item>
        <item name="expandedTitleTextColor">?android:attr/colorForeground</item>
        <item name="collapsedTitleTextColor">?android:attr/colorForeground</item>
        <item name="titleCollapseMode">scale</item>
        <item name="title">@string/title</item>
    </style>

    <style name="SmallIcon">
        <item name="android:adjustViewBounds">true</item>
        <item name="tint">?android:attr/colorForeground</item>
        <item name="android:layout_height">20sp</item>
        <item name="android:layout_width">20sp</item>
    </style>

    <style name="CustomBottomSheetDialog" parent="@style/ThemeOverlay.MaterialComponents.BottomSheetDialog">
        <item name="bottomSheetStyle">@style/CustomBottomSheet</item>
        <item name="backgroundTint">@android:color/transparent</item>
    </style>

    <style name="BottomSheetDialogFull" parent="@style/ThemeOverlay.MaterialComponents.BottomSheetDialog">
        <item name="bottomSheetStyle">@style/BottomSheetFull</item>
    </style>

    <style name="CustomBottomSheet" parent="Widget.MaterialComponents.BottomSheet">
        <item name="shapeAppearanceOverlay">@style/CustomShapeAppearanceBottomSheetDialog</item>
