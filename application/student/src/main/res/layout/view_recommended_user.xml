                            android:checked="true"
                            android:padding="@dimen/text_container_margin"
                            android:text="Man"
                            app:checkedIconEnabled="false"
                            app:chipBackgroundColor="?attr/colorErrorContainer"
                            app:chipEndPadding="@dimen/text_container_margin"
                            app:chipIcon="@drawable/like_icon"
                            app:chipIconEnabled="true"
                            app:chipMinHeight="25dp"
                            app:chipMinTouchTargetSize="0dp"
                            app:chipStartPadding="@dimen/text_container_margin" />

