<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate
            android:fromDegrees="-45"
            android:pivotX="50%"
            android:pivotY="50%">
            <shape
                android:shape="line">
                <stroke
                    android:width="250dp"
                    android:color="#FFFFFF"
                    android:dashWidth="10dp"
                    android:dashGap="10dp"/>
            </shape>
        </rotate>
    </item>
</layer-list>